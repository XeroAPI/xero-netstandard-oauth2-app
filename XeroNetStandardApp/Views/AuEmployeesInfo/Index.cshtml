@model dynamic

<h2>Xero AU Payroll Employees</h2>

<table class="table table-responsive fixed-table-body">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Job Title</th>
    <th>Gender</th>
    <th>Status</th>
    <th>Employee ID</th>
  </tr>
  @foreach (var employee in Model) {
    <tr>
      <td>@employee.FirstName</td>
      <td>@employee.LastName</td>
      <td>@employee.JobTitle</td>
      <td>@employee.Gender</td>
      <td>@employee.Status</td>
      <td>@employee.EmployeeID</td>
      <td>
        <button class="btn btn-primary" onclick="location.href='@Url.Action("FindEmployee", "AuEmployeesInfo", new { employeeId = (Guid) @employee.EmployeeID } )'">Find Employee by ID</button>
      </td>
    </tr>
  }
</table>
<br>
<p><b>Raw JSON Response:</b></p>
<textarea class="form-control" readonly style="height: 350px">
@ViewBag.jsonResponse;
</textarea>
<br>
<p><b>Code Example:</b></p>
<p>Get all AU Employees</p>
<pre>
// Assuming accessToken and xeroTenantId has been provided
var PayrollAuApi = new PayrollAuApi();
var response = await PayrollAuApi.GetEmployeesAsync(accessToken, xeroTenantId);
return response._Employees;
</pre>
<br>
<p>
  <button class="btn btn-link" onclick="location.href='@Url.Action("Index", "Home")'">Home</button>
</p>
